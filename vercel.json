{
    "version": 2,
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "apps/api/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/apps/api/src/index.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/",
            "dest": "/apps/web/index"
        },
        {
            "src": "/(.*)",
            "dest": "/apps/web/$1"
        }
    ]
}